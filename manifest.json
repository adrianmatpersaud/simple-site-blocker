{
    "manifest_version": 3,
    "name": "Site Blocker",
    "version": "0.0.1",
    "action": {
      "default_popup": "index.html",
      "default_icon":  "icon128.png"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ]
}